
class Animal:
    alive=True
    def eat(self):
        print("This animal is eating")
    def sleep(self):
        print("The animal is sleeping")
class Rabbit(Animal):
    def run(self):
        print("rabbit is running")
class Fish(Animal):
    pass
class Hawk(Animal):
    pass
rabbit=Rabbit()
fish=Fish()
hawk=Hawk()
fish.eat()
