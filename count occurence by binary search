#include <stdio.h>

void sort(int a[], int n) {
    for (int i = 0; i < n - 1; i++)
        for (int j = 0; j < n - i - 1; j++)
            if (a[j] > a[j + 1]) {
                int t = a[j];
                a[j] = a[j + 1];
                a[j + 1] = t;
            }
}

int firstOcc(int a[], int n, int val) {
    int low = 0, high = n - 1, mid, ans = -1;

    while (low <= high) {
        mid = (low + high) / 2;
        if (a[mid] == val) {
            ans = mid;
            high = mid - 1;   
        } else if (a[mid] > val)
            high = mid - 1;
        else
            low = mid + 1;
    }
    return ans;
}

int lastOcc(int a[], int n, int val) {
    int low = 0, high = n - 1, mid, ans = -1;

    while (low <= high) {
        mid = (low + high) / 2;
        if (a[mid] == val) {
            ans = mid;
            low = mid + 1; 
        } else if (a[mid] > val)
            high = mid - 1;
        else
            low = mid + 1;
    }
    return ans;
}

int main() {
    int x, val;
    scanf("%d %d", &x, &val);

    int a[x];

    for (int i = 0; i < x; i++)
        scanf("%d", &a[i]);

    sort(a, x);

    int first = firstOcc(a, x, val);
    int last = lastOcc(a, x, val);

    if (first == -1)
        printf("Occurrences: 0\n");
    else
        printf("Occurrences: %d\n", last - first + 1);

    return 0;
}
