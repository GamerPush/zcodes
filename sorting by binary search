#include<stdio.h>
int main(){
int val,x,temp;
scanf("%d %d",&x,&val);
int a[x];
for(int i=0;i<x;i++){
    scanf("%d",a[i]);
}

for (int i = 0; i < x - 1; i++) {
    for (int j = 0; j < x - i - 1; j++) {
        if (a[j] > a[j + 1]) {
            temp = a[j];
            a[j] = a[j + 1];
            a[j + 1] = temp;
        }
    }
}

int mid,low=0,high=x-1;
while(low<=high){
mid=low+high/2;

if(a[mid]==val){
return mid;
}

else if(a[mid]>val){
high=mid-1;
}

else{
    low=mid+1;
}

}
return -1;
}
